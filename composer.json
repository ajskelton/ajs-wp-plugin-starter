{
    "name"			: "ajskelton/wordpress-starter-plugin",
    "description"	: "Starter Sandbox plugin. Use this plugin for all of the testing.",
    "type"			: "project",
    "keywords"		: ["wordpress", "wp", "plugin"],
    "homepage"		: "https://anthonyskelton.com",
    "license"		: "GPL-2.0+",
    "authors": [
        {
            "name": "Anthony Skelton",
            "email": "anthony@anthonyskelton.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
    	"raveren/kint": "~1.0",
    	"filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [],
        "files": [
            "src/support/exceptions.php",
            "src/sandbox.php"
        ]
    },
    "extra": {
    	"branch-alias": {
    		"dev-master": "1.0-dev"
    	}
    },
    "config": {
    	"vendor-dir": "assets/vendor"
    },
    "minimum-stability": "dev"
}